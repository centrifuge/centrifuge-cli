{
  "name": "@centrifuge-cli/plugin-migration",
  "description": "Centrifuge CLI plugin used for migrating accounts from Centrifuge standalone chain to a Polkadot/Kusama parachain.",
  "version": "0.1.0",
  "author": "Centrifuge Foundation <admin@centrifuge.io>",
  "homepage": "https://github.com/centrifuge/centrifuge-cli#README.md",
  "bugs": {"url": "https://github.com/centrifuge/centrifuge-cli/issues"},
  "license": "Apache-2.0",
  "keywords": [
    "centrifuge",
    "centrifuge-cli",
    "plugin-migration"
  ],
  "type": "module",
  "main": "dist/index.js",
  "exports": {
      "./*": "./dist/*"
  },
  "typesVersions": {
      "*": { 
          "*": ["dist/*"]
      }
  },
  "repository": {
    "type": "git",
    "directory": "packages/plugins/migration",
    "url": "https://github.com/centrifuge/centrifuge-cli.git"
  },
  "engines": {
    "node": ">=14.17.4"
  },
  "scripts": {
    "clean": "rimraf dist node_modules tsconfig.tsbuildinfo .nyc_output",
    "prepack": "yarn build",
    "build": "yarn clean && yarn compile",
    "compile": "tsc --build && cp \"./package.json\" ./dist/",
    "test": "echo 'no test'",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings=0"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@polkadot/api": "^6.5.2",
    "@centrifuge-cli/core": "0.1.0",
    "@centrifuge-cli/dispatcher": "0.1.0",
    "json-bigint": "^1.0.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.0"
  },
  "oclif": {
    "commands": "./dist/commands",
    "bin": "migration",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "files": [
    "/dist"
  ]
}